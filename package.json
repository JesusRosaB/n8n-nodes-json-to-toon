{
  "name": "n8n-nodes-json-to-toon",
  "version": "1.0.0",
  "description": "n8n nodes for converting between JSON and TOON (Token-Oriented Object Notation) format",
  "keywords": [
    "n8n",
    "n8n-community-node-package",
    "toon",
    "json",
    "llm",
    "token-optimization"
  ],
  "license": "MIT",
  "author": {
    "name": "Tu Nombre",
    "email": "tu@email.com"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/tu-usuario/n8n-nodes-json-to-toon.git"
  },
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "tsc && gulp build:icons",
    "dev": "tsc --watch",
    "lint": "eslint src --ext .ts",
    "prepublishOnly": "npm run build"
  },
  "files": [
    "dist"
  ],
  "n8n": {
    "n8nNodesApiVersion": 1,
    "nodes": [
      "dist/nodes/JsonToToon/JsonToToon.node.js",
      "dist/nodes/ToonToJson/ToonToJson.node.js"
    ],
    "credentials": []
  },
  "devDependencies": {
    "@types/node": "^20.10.0",
    "gulp": "^4.0.2",
    "n8n-workflow": "^1.20.0",
    "typescript": "^5.3.0"
  },
  "peerDependencies": {
    "n8n-workflow": "*"
  }
}
